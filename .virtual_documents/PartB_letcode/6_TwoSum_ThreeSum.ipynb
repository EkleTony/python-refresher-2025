st = "Ababa"
def isPalindrome(st):
    """
        1. to lower
        2. remove punction or numbers
        3. compare with reverse
    """
    lst = []
    st = st.lower()
    for char in st:
        if char.isalnum():
            lst.append(char)
    newstr = ''.join(lst)
    return newstr[::1] == newstr[::-1]

isPalindrome(st)
    



